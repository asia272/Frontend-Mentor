{
  "rewrites": [
    {
      "source": "/static-projects/(.*)",
      "destination": "/public/static-projects/$1"
    },
    {
      "source": "/react-projects/:project",
      "destination": "/react-projects/:project/dist/index.html"
    },
    {
      "source": "/react-projects/:project/(.*)",
      "destination": "/react-projects/:project/dist/$1"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
